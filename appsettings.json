{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "SimpleProxy"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://*:80;https://*:443",
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "/app/keys/chain.pem",
        "KeyPath": "/app/keys/chain.pem"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "dummy": {
        "ClusterId": "dummy",
        "Match": {
          "Path": "{**catch-all}",
          "Hosts": [ "dummy.example.com"]
        }
      }
    },
    "Clusters": {
      "dummy": {
        "Destinations": {
          "dummy": {
            "Address": "http://localhost:8080"
          }
        }
      }
    }
  }
}
